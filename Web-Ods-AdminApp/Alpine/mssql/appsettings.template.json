{
  "AppSettings": {
    "AppStartup": "OnPrem",
    "DatabaseEngine": "SqlServer",
    "ApplicationInsightsInstrumentationKey": "",
    "XsdFolder": "/app/Schema",
    "DefaultOdsInstance": "EdFi ODS",
    "ProductionApiUrl": "$API_INTERNAL_URL",
    "ApiExternalUrl": "$API_EXTERNAL_URL",
    "SystemManagedSqlServer": "true",
    "DbSetupEnabled": "false",
    "SecurityMetadataCacheTimeoutMinutes": "0",
    "ApiStartupType": "$API_MODE",
    "LocalEducationAgencyTypeValue": "Local Education Agency",
    "SchoolTypeValue": "School",
    "BulkUploadHashCache": "/app/BulkUploadHashCache/",
    "IdaAADInstance": "",
    "IdaClientId": "",
    "IdaClientSecret": "",
    "IdaTenantId": "",
    "IdaSubscriptionId": "",
    "AwsCurrentVersion": "1.0",
    "EncryptionProtocol": "AES",
    "EncryptionKey": "$ENCRYPTION_KEY",
    "PathBase": "$ADMINAPP_VIRTUAL_NAME"
  },
  "ConnectionStrings": {
    "Admin": "Data Source=$SQLSERVER_DATASOURCE;Initial Catalog=EdFi_Admin;User Id=$SQLSERVER_USER;Password=$SQLSERVER_PASSWORD;Application Name=EdFi.Ods.AdminApp;Integrated Security=false;",
    "Security": "Data Source=$SQLSERVER_DATASOURCE;Initial Catalog=EdFi_Security;User Id=$SQLSERVER_USER;Password=$SQLSERVER_PASSWORD;Application Name=EdFi.Ods.AdminApp;Integrated Security=false;",
    "ProductionOds": "Data Source=$SQLSERVER_DATASOURCE;Initial Catalog=EdFi_Ods;User Id=$SQLSERVER_USER;Password=$SQLSERVER_PASSWORD;Application Name=EdFi.Ods.AdminApp;Integrated Security=false;"
  },
  "Log4NetCore": {
    "Log4NetConfigFileName": "./log4net.config"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
